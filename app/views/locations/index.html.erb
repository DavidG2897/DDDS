<p id="notice"><%= notice %></p>

<h1>Locations</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Registered at</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locations.each do |location| %>
      <tr>
        <td><%= location.id %></td>
        <td><%= location.created_at %></td>
        <td><%= location.lat %></td>
        <td><%= location.long %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back to emergencies', emergencies_path %>
